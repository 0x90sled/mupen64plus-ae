            android:defaultValue="@string/gliden64_multisampling_default"
            android:entries="@array/gliden64_multisampling_entries"
            android:entryValues="@array/gliden64_multisampling_values"
            android:key="MultiSampling"
            android:summary="@string/selectedValue"
            android:title="@string/gliden64_multisampling_title" />
        <paulscode.android.mupen64plusae.preference.CompatListPreference
            android:defaultValue="@string/gliden64_bilinear_default"
            android:entries="@array/gliden64_bilinear_entries"
            android:entryValues="@array/gliden64_bilinear_values"
            android:key="bilinearMode"
            android:summary="@string/selectedValue"
            android:title="@string/gliden64_bilinear_mode_title" />
        <paulscode.android.mupen64plusae.preference.SeekBarPreference
            android:defaultValue="0"
            android:key="MaxAnisotropy"
            android:title="@string/gliden64_max_anisotropic_filtering_title"
            mupen64:maximumValue="16"
            mupen64:minimumValue="0"
            mupen64:stepSize="1"
            mupen64:saveType="string"
            mupen64:units="x" />
        <paulscode.android.mupen64plusae.preference.SeekBarPreference
            android:defaultValue="256"
            android:key="CacheSize"
            android:title="@string/gliden64_tx_cache_title"
            mupen64:maximumValue="512"
            mupen64:minimumValue="50"
            mupen64:stepSize="1"
            mupen64:saveType="string"
            mupen64:units="MB" />
    </android.support.v7.preference.PreferenceCategory>
    <android.support.v7.preference.PreferenceCategory
