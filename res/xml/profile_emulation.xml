        <paulscode.android.mupen64plusae.preference.StringCheckBoxPreference
            android:defaultValue="false"
            android:key="EnableHWLighting"
            android:title="@string/gliden64_enable_hw_lighting_title" />
        <paulscode.android.mupen64plusae.preference.StringCheckBoxPreference
            android:defaultValue="true"
            android:key="EnableShaderStorage"
            android:title="@string/gliden64_enable_shader_storage_title" />

        
        <paulscode.android.mupen64plusae.preference.StringCheckBoxPreference
