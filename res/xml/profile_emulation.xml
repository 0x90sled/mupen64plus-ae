            android:key="txFilterMode"
            android:summary="@string/selectedValue"
            android:title="@string/gliden64_tx_filter_mode_title" />
        <paulscode.android.mupen64plusae.preference.CompatListPreference
            android:defaultValue="@string/gliden64_texture_enhancement_default"
            android:entries="@array/gliden64_texture_enhancement_entries"
