        <paulscode.android.mupen64plusae.preference.StringCheckBoxPreference
            android:defaultValue="false"
            android:key="EnableCopyColorFromRDRAM"
            android:title="@string/gliden64_enable_color_buffer_from_RDRAM_title" />
    </android.support.v7.preference.PreferenceCategory>
    <android.support.v7.preference.PreferenceCategory
        android:key="categoryGliden64TextureFiltering"
        android:title="@string/gliden64_category_texture_filtering_title" >
        <paulscode.android.mupen64plusae.preference.CompatListPreference
            android:defaultValue="@string/gliden64_texture_filter_default"
